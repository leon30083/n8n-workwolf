# Environment Variables for n8n-workwolf
# Copy this file to .env and fill in the actual values

# ============================================
# Everything-Claude-Code MCP Servers
# ============================================

# GitHub Token (for github MCP)
# Get from: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# GLM API Key (for web-search-prime, web-reader)
# Get from: https://open.bigmodel.cn/usercenter/apikeys
GLM_API_KEY=your_glm_api_key_here

# ZAI API Key (for zai-mcp-server)
# Get from: https://open.bigmodel.cn/usercenter/apikeys
ZAI_API_KEY=your_zai_api_key_here

# ============================================
# n8n-workwolf MCP Servers
# ============================================

# n8n API Configuration (for n8n-mcp)
N8N_API_URL=http://localhost:5678
N8N_API_KEY=your_n8n_api_key_here

# Project Path (for skills-mcp)
PROJECT_PATH=E:/User/GitHub/n8n-workwolf

# ============================================
# Optional MCP Servers
# ============================================

# Firecrawl (web scraping)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Supabase (database operations)
SUPABASE_PROJECT_REF=your_supabase_project_ref_here

# Vercel (deployments)
VERCEL_TOKEN=your_vercel_token_here

# Railway (deployments)
RAILWAY_TOKEN=your_railway_token_here

# Filesystem MCP (default path)
FILESYSTEM_PATH=E:/User/GitHub
